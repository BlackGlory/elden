{
  "manifest_version": 2
, "name": "Elden"
, "author": "BlackGlory"
, "version": "0.1.5"
, "description": "Modern ECMAScript loader."
, "background": {
    "scripts": ["browser-polyfill.min.js", "background.js"]
  , "persistent": true
  }
, "browser_action": {
    "default_popup": "options.html"
  }
, "options_ui": {
    "page": "options.html"
  , "chrome_style": false
  , "open_in_tab": true
  }
, "permissions": [
    "webNavigation"
  , "webRequest"
  , "webRequestBlocking"
  , "<all_urls>"
  ]
}
